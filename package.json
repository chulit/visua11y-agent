{
  "name": "visua11y-agent",
  "type": "module",
  "version": "1.4.1",
  "description": "Visua11y Agent: Accessibility Website Widget",
  "main": "dist/visua11y-agent.cjs.js",
  "module": "dist/visua11y-agent.esm.js",
  "unpkg": "dist/visua11y-agent.umd.js",
  "jsdelivr": "dist/visua11y-agent.umd.js",
  "files": [
    "dist",
    "types",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "watch": "node esbuild.config.js --watch",
    "build": "node esbuild.config.js --minify",
    "lint": "eslint src --ext .ts,.js",
    "demo": "npm run build && node scripts/demo-server.js",
    "demo:serve": "node scripts/demo-hot.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "release": "semantic-release",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:build-demo": "node scripts/build-demo.js",
    "docs:publish": "npm run build && npm run docs:build-demo && npm run docs:build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chulit/visua11y-agent.git"
  },
  "author": "Chulit",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chulit/visua11y-agent/issues"
  },
  "homepage": "https://chulit.github.io/visua11y-agent/",
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.6",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@vitest/ui": "^4.0.5",
    "esbuild": "^0.21.5",
    "eslint": "^9.38.0",
    "html-minifier": "^4.0.0",
    "jsdom": "^27.0.1",
    "prettier": "^3.6.2",
    "semantic-release": "^24.2.9",
    "svgo": "^4.0.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.1",
    "vite-plugin-html": "^3.2.2",
    "vitepress": "^1.6.4",
    "vitest": "^4.0.5"
  }
}
